<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:background="@color/white"
    android:orientation="vertical">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:minHeight="?attr/actionBarSize"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">
        <ImageButton
            android:id="@+id/vector1"
            android:layout_width="10.74dp"
            android:layout_height="18.76dp"
            android:layout_marginBottom="0dp"
            android:background="@drawable/vector"
            />
    </androidx.appcompat.widget.Toolbar>

    <TextView
        android:id="@+id/what_is_your_body_shape"
        android:layout_width="wrap_content"
        android:layout_height="28dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="58dp"

        android:fontFamily="@font/montserrat"
        android:text="What's your body shape?"
        android:textColor="@color/black"
        android:textSize="20dp"
        />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="51dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        >


        <ImageButton
            android:id="@+id/Hourglass_pic"
            android:layout_width="86dp"
            android:layout_height="164dp"
            android:layout_marginLeft="21dp"
            android:layout_marginTop="0dp"
            android:layout_marginRight="0dp"
            android:layout_marginBottom="0dp"
            android:background="@drawable/image_5"
            />



    <ImageButton
        android:id="@+id/Round_pic"
        android:layout_width="86dp"
        android:layout_height="164dp"
        android:layout_marginLeft="37dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="37dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/image_6"
        />


    <ImageButton
        android:id="@+id/Triangle_pic"
        android:layout_width="86dp"
        android:layout_height="164dp"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="21dp"
        android:background="@drawable/image_7"
        android:gravity="center_horizontal"
         />


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:gravity="center_horizontal">

     <TextView
        android:id="@+id/Hourglass"
        android:layout_width="86dp"
        android:layout_height="22dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="0dp"
        android:fontFamily="@font/montserrat"
        android:gravity="center_horizontal"
        android:text="Hourglass"
        android:textColor="@color/black"
        android:textSize="14dp"
        android:layout_marginLeft="21dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp"
         android:singleLine="true"
         android:ellipsize="marquee"

        android:marqueeRepeatLimit="marquee_forever"
        android:focusable="true"
        android:focusableInTouchMode="true"
        />

    <TextView
        android:id="@+id/Round"
        android:layout_width="86dp"
        android:layout_height="22dp"
        android:layout_marginStart="37dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="0dp"
        android:fontFamily="@font/montserrat"
        android:gravity="center_horizontal"
        android:text="Round"
        android:textColor="@color/black"
        android:textSize="14dp"
         />

    <TextView
        android:id="@+id/Triangle"
        android:layout_width="86dp"
        android:layout_height="22dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:fontFamily="@font/montserrat"
        android:gravity="center_horizontal"
        android:text="Triangle"
        android:textColor="@color/black"
        android:textSize="14dp"
         />
    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_marginTop="0dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/shape1"
            android:layout_width="85dp"
            android:layout_height="5dp"
            android:layout_marginTop="0dp"
            />

        <ImageView
            android:id="@+id/shape2"
            android:layout_width="85dp"
            android:layout_height="5dp"
            android:layout_marginLeft="37dp"
            android:layout_marginTop="0dp"
            android:layout_marginRight="37dp" />

        <ImageView
            android:id="@+id/shape3"
            android:layout_width="85dp"
            android:layout_height="5dp"
            android:layout_marginTop="0dp"
            />
    </LinearLayout>








    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="34dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal">


        <ImageButton
            android:id="@+id/Rectangle_pic"
            android:layout_width="86dp"
            android:layout_height="164dp"
            android:layout_marginStart="76dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="0dp"
            android:background="@drawable/image_8"
            android:gravity="center_horizontal"

            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/Pear_pic"
            android:layout_width="86dp"
            android:layout_height="164dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="76dp"
            android:layout_marginBottom="0dp"
            android:background="@drawable/image_9"
            android:gravity="center_horizontal"

            tools:ignore="MissingConstraints" />

    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/Rectangle"
            android:layout_width="86dp"
            android:layout_height="22dp"
            android:layout_marginStart="76dp"
            android:layout_marginEnd="24dp"

            android:layout_marginBottom="0dp"
            android:fontFamily="@font/montserrat"
            android:gravity="center_horizontal"
            android:text="Rectangle"
            android:textColor="@color/black"
            android:textSize="14dp"
           />



        <TextView
            android:id="@+id/Pear"
            android:layout_width="86dp"
            android:layout_height="22dp"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="76dp"
            android:layout_marginBottom="0dp"
            android:fontFamily="@font/montserrat"
            android:gravity="center_horizontal"

            android:text="Pear"
            android:textColor="@color/black"
            android:textSize="14dp"
           />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_marginTop="0dp"
        android:gravity="center_horizontal"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/shape4"
            android:layout_width="85dp"
            android:layout_height="5dp"
            android:layout_marginLeft="76dp"
            android:layout_marginTop="0dp"
            android:layout_marginRight="24dp" />

        <ImageView
            android:id="@+id/shape5"
            android:layout_width="85dp"
            android:layout_height="5dp"
            android:layout_marginRight="76dp"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="24dp"
            />
    </LinearLayout>





    <ImageButton
        android:id="@+id/next"
        android:layout_width="124dp"
        android:layout_height="40dp"
        android:layout_marginTop="51dp"
        android:layout_marginStart="124dp"
        android:layout_marginEnd="124dp"
        android:layout_marginBottom="48dp"
        android:background="@drawable/frame_434"
        android:gravity="center_horizontal"
        />


</LinearLayout>
